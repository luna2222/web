	<script>
		var grade;
		var score=prompt("홍길동 님의 점수를 입력",100);
		score=parseInt(socre); // 문자열을 숫자로 바꿈
		if(score>=90) //90점이상
			grade="A";
		else if(score >=80 )
			grade=" B";
		else if(score >=70 )
			grade="C ";
		else if(score >=60 )
			grade="D ";
		else 
			grade=" F";
		document.write(score+"는"+grade+"입니다 .<br>")
	</script>

